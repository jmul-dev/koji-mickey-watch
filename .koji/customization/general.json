{
  "general": {
    "name": "Mickey Watch",
    "fontFamily": "https://fonts.googleapis.com/css?family=DM+Sans",
    "backgroundImage": "https://images.pexels.com/photos/1341279/pexels-photo-1341279.jpeg?fit=cover",
    "clockFace": "https://images.koji-cdn.com/4ed9c8e0-dd4b-42d2-b650-82e34bcf88ae/mickey2.png",
    "hourHand": "https://images.koji-cdn.com/4ed9c8e0-dd4b-42d2-b650-82e34bcf88ae/9c1a977dd3d241b4b9968894c5267244.png",
    "minuteHand": "https://images.koji-cdn.com/4ed9c8e0-dd4b-42d2-b650-82e34bcf88ae/61392bbb86dc42e68c2713b5d8ecc2c4.png",
    "primaryColor": "#ffffff",
    "backgroundColor": "#000000",
    "secondHandColor": "#ff0000",
    "handsCoverColor": "#ffd905",
    "radialGradientColor": "#909090"
  },
  "@@editor": [
    {
      "key": "general",
      "name": "Clock Settings",
      "icon": "‚è∞",
      "source": "general.json",
      "fields": [
        {
          "key": "name",
          "name": "App name"
        },
        {
          "key": "fontFamily",
          "name": "Font Family",
          "description": "Google fonts link to font to use on page"
        },
        {
          "key": "backgroundImage",
          "name": "Background Image",
          "description": "URL of Image to place as background",
          "type": "image",
          "typeOptions": {
            "fit": "cover"
          }
        },
        {
          "key": "clockFace",
          "name": "Clock Face Image",
          "description": "URL of Image to place as clock face",
          "type": "image"
        },
        {
          "key": "hourHand",
          "name": "Hour Hand Image",
          "type": "image",
          "description": "URL of image to put on Hour Hand"
        },
        {
          "key": "minuteHand",
          "name": "Minute Hand Image",
          "type": "image",
          "description": "URL of image to put on Minute Hand"
        },
        {
          "key": "primaryColor",
          "name": "Primary Color",
          "description": "Primary color",
          "type": "color"
        },
        {
          "key": "backgroundColor",
          "name": "Background Color",
          "description": "Background color",
          "type": "color"
        },
        {
          "key": "secondHandColor",
          "name": "Second Hand Color",
          "description": "Color to use on Second Hand",
          "type": "color"
        },
        {
          "key": "handsCoverColor",
          "name": "Hands Cover Color",
          "description": "Color to use on Hands Cover",
          "type": "color"
        },
        {
          "key": "radialGradientColor",
          "name": "Radial Gradient Color",
          "description": "Start Color for the radial gradient behind clock face",
          "type": "color"
        }
      ]
    }
  ]
}
